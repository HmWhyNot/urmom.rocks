                                {
                                    CoworkerID = pick.GetValue("Pick_Verification_By").ToString().Trim().ToUpper(),
                                    FirstName = pick.GetValue("Pick_Verification_By_FirstName").ToString().FormatName(),
                                    LastName = pick.GetValue("Pick_Verification_By_LastName").ToString().FormatName()
                                };
                            }),
                            PickVerifiedDate = r.GetValue("Pick_Verification_Date").ToNullableDateTime(),
                            PickVerificationNote = r.GetValue("Pick_Verification_Note").ToNullableString(),
                            ShippingMethod = r.GetValue("Ship_Method").ToNullableResult(r, (pick) =>
                            {
                                return new ShipMethod
                                {
                                    Code = pick.GetValue("Ship_Method").ToString(),
                                    Description = pick.GetValue("Ship_Method_Description").ToString()
                                };
                            }),
                            SLAOriginalDate = r.GetValue("SLA_Date").ToNullableDateTime(),
                            SLAOverrideDate = r.GetValue("SLA_Override_Date").ToNullableDateTime(),
                            SLAOverrideBy = r.GetValue("SLA_Override_By").ToNullableResult(r, (pick) =>
                            {
                                return new Coworker
                                {
                                    CoworkerID = pick.GetValue("SLA_Override_By").ToNullableString()?.Trim().ToUpper(),
                                    FirstName = pick.GetValue("SLA_Override_By_FirstName").ToNullableString()?.FormatName(),
                                    LastName = pick.GetValue("SLA_Override_By_LastName").ToNullableString()?.FormatName()
                                };
                            }),
                            SLAOverrideOn = r.GetValue("SLA_Override_On").ToNullableDateTime(),
                            SLADays = r.GetValue("SLA_Days").ToNullableInt(),
                            EstimatedDueDateOriginalDate = r.GetValue("Estimated_Due_Date").ToNullableDateTime(),
                            EstimatedDueDateOverrideDate = r.GetValue("Estimated_Due_Date_Override_Date").ToNullableDateTime(),
                            EstimatedDueDateOverrideBy = r.GetValue("Estimated_Due_Date_Override_By").ToNullableResult(r, (pick) =>
                            {
                                return new Coworker
                                {
                                    CoworkerID = pick.GetValue("Estimated_Due_Date_Override_By").ToNullableString()?.Trim().ToUpper(),
                                    FirstName = pick.GetValue("Estimated_Due_Date_Override_By_FirstName").ToNullableString()?.FormatName(),
                                    LastName = pick.GetValue("Estimated_Due_Date_Override_By_LastName").ToNullableString()?.FormatName()
                                };
                            }),
                            EstimatedDueDateOverrideOn = r.GetValue("Estimated_Due_Date_Override_On").ToNullableDateTime(),
                            TrueAging = r.GetValue("True_Aging").ToNullableInt(),
                            TotalFlaggedDays = r.GetValue("Flags_Days").ToNullableInt(),
                            TotalHoldDays = r.GetValue("Holds_Days").ToNullableInt(),
                            StartDate = r.GetValue("Start_Date").ToNullableDateTime(),
                            StartDateSetBy = r.GetValue("Start_Date_By").ToNullableResult(r, (pick) =>
                            {
                                return new Coworker
                                {
                                    CoworkerID = pick.GetValue("Start_Date_By").ToNullableString()?.Trim().ToUpper(),
                                    FirstName = pick.GetValue("Start_Date_By_FirstName").ToNullableString()?.FormatName(),
                                    LastName = pick.GetValue("Start_Date_By_LastName").ToNullableString()?.FormatName()
                                };
                            }),
                            StartDateSetOn = r.GetValue("Start_Date_On").ToNullableDateTime(),
                            TotalServices = r.GetValue("Total_Services_Count").ToNullableInt(),
                            TotalManagedServices = r.GetValue("Total_Managed_Services_Count").ToNullableInt(),
                            SpecialOpsCount = r.GetValue("Spec_Ops").ToNullableInt()
                        }).ToList()
                    };
                }
                return new FrameworkSearchResponse
                {
                    Success = true,
                    Message = "No picks found.",
                    TotalResults = 0,
                    Results = new List<Pick>()
                };
            }
            catch(Exception e)
            {
                return new FrameworkResponse(e);
            }
        }

        public static FrameworkResponse GetTemplates(int? Limit = null, int? Offset = null, string Sort = null, string Order = null, bool? IncludeArchived = null, string Search = null)
        {
            try
            {
                using (SqlConnection Connection = new SqlConnection(SQL.GetConnectionString("PrimaryDB")))
                {
                    using (SqlCommand Command = new SqlCommand("sp_ccCBCB_Get_Templates_NEW", Connection))
                    {
                        if (Connection.State != ConnectionState.Open) { Connection.Open(); }
                        Command.CommandType = CommandType.StoredProcedure;
                        Command.Parameters.AddWithValue("@Limit", Limit.HasValue ? (object)Limit.Value : DBNull.Value);
                        Command.Parameters.AddWithValue("@Offset", Offset.HasValue ? (object)Offset.Value : DBNull.Value);
                        Command.Parameters.AddWithValue("@IncludeArchived", IncludeArchived.HasValue ? (object)IncludeArchived.Value : DBNull.Value);
                        Command.Parameters.AddWithValue("@Search", !Search.IsNullOrEmptyOrWhiteSpace() ? (object)string.Join(",", Search.Split(new string[] { " " }, StringSplitOptions.RemoveEmptyEntries)) : DBNull.Value);
                        Command.Parameters.AddWithValue("@ORDER_TEXT", Sort != null && Order != null ? (object)$"{Sort} {Order}" : DBNull.Value);
                        using (SqlDataReader DataReader = Command.ExecuteReader())
                        {
                            if (DataReader.HasRows)
                            {
                                int TotalResults = 0;
                                List<ColorBrandingTemplate> Output = new List<ColorBrandingTemplate>();
                                while (DataReader.Read())
                                {
                                    TotalResults = Convert.ToInt32(DataReader.GetValue(DataReader.GetOrdinal("TotalResults")));
                                    Output.Add(new ColorBrandingTemplate
                                    {
                                        ID = Convert.ToInt64(DataReader.GetValue(DataReader.GetOrdinal("ID"))),
                                        Name = DataReader.GetValue(DataReader.GetOrdinal("Name")).ToString(),
                                        Customer = new Customer
                                        {
                                            ID = DataReader.GetValue(DataReader.GetOrdinal("Customer")).ToString(),
                                            Name = DataReader.GetValue(DataReader.GetOrdinal("Customer_Name")).ToString(),
                                            PrimarySalesRep = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_EmpID")) != DBNull.Value ? new Coworker
                                            {
                                                CoworkerID = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_EmpID")).ToString().Trim().ToUpper(),
                                                FirstName = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_FirstName")).ToString().FormatName(),
                                                LastName = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_LastName")).ToString().FormatName(),
                                                Extension = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_Extension")) != DBNull.Value ? Convert.ToInt32(DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_Extension"))) : (int?)null,
                                                Email = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_Email")) != DBNull.Value ? DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_Email")).ToString() : null,
                                                Phone = DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_Phone")) != DBNull.Value ? DataReader.GetValue(DataReader.GetOrdinal("Customer_Primary_Sales_Rep_Phone")).ToString() : null
                                            } : null
                                        },
                                        Status = Convert.ToInt64(DataReader.GetValue(DataReader.GetOrdinal("Status"))) == 1 ? Statuses.Pending : Convert.ToInt64(DataReader.GetValue(DataReader.GetOrdinal("Status"))) == 2 ? Statuses.Approved : Convert.ToInt64(DataReader.GetValue(DataReader.GetOrdinal("Status"))) == 3 ? Statuses.Archived : Statuses.UNKNOWN,
                                        Duration = DataReader.GetValue(DataReader.GetOrdinal("Duration")) != DBNull.Value ? TimeSpan.FromSeconds(Convert.ToDouble(DataReader.GetValue(DataReader.GetOrdinal("Duration")))) : (TimeSpan?)null,
                                        ServiceClass = new ServiceClass
                                        {
                                            ID = Convert.ToInt64(DataReader.GetValue(DataReader.GetOrdinal("Service_Class"))),
                                            Name = DataReader.GetValue(DataReader.GetOrdinal("Service_Class_Name")).ToString()
                                        },
                                        RITM = DataReader.GetValue(DataReader.GetOrdinal("RITM")) != DBNull.Value ? DataReader.GetValue(DataReader.GetOrdinal("RITM")).ToString() : null,
                                        CreationOrder = DataReader.GetValue(DataReader.GetOrdinal("Creation_Order")) != DBNull.Value ? new Order(DataReader.GetValue(DataReader.GetOrdinal("Creation_Order")).ToString()) : null,
                                        Dimensions = DataReader.GetValue(DataReader.GetOrdinal("Dimensions_Width")) != DBNull.Value && DataReader.GetValue(DataReader.GetOrdinal("Dimensions_Height")) != DBNull.Value ? new SizeF(Convert.ToSingle(DataReader.GetValue(DataReader.GetOrdinal("Dimensions_Width"))), Convert.ToSingle(DataReader.GetValue(DataReader.GetOrdinal("Dimensions_Height")))) : (SizeF?)null,
                                        IsDynamic = Convert.ToBoolean(DataReader.GetValue(DataReader.GetOrdinal("IsDynamic"))),
                                        CreatedBy = new Coworker
                                        {
                                            CoworkerID = DataReader.GetValue(DataReader.GetOrdinal("Created_By")).ToString().Trim().ToUpper(),
                                            FirstName = DataReader.GetValue(DataReader.GetOrdinal("Created_By_FirstName")).ToString().FormatName(),
                                            LastName = DataReader.GetValue(DataReader.GetOrdinal("Created_By_LastName")).ToString().FormatName()
                                        },
                                        CreatedDate = Convert.ToDateTime(DataReader.GetValue(DataReader.GetOrdinal("Created_Date"))),
                                        LastModifiedBy = DataReader.GetValue(DataReader.GetOrdinal("Last_Modified_By")) != DBNull.Value ? new CDW.Coworker
                                        {
                                            CoworkerID = DataReader.GetValue(DataReader.GetOrdinal("Last_Modified_By")).ToString().Trim().ToUpper(),
                                            FirstName = DataReader.GetValue(DataReader.GetOrdinal("Last_Modified_By_FirstName")).ToString().FormatName(),
                                            LastName = DataReader.GetValue(DataReader.GetOrdinal("Last_Modified_By_LastName")).ToString().FormatName()
                                        } : null,
                                        LastModifiedDate = DataReader.GetValue(DataReader.GetOrdinal("Last_Modified_Date")) != DBNull.Value ? Convert.ToDateTime(DataReader.GetValue(DataReader.GetOrdinal("Last_Modified_Date"))) : (DateTime?)null
                                    });
                                }
                                return new FrameworkSearchResponse
                                {
                                    Success = true,
                                    Message = "Successfully retrieved templates.",
                                    TotalResults = TotalResults,
                                    Results = Output
                                };
                            }
                            return new FrameworkSearchResponse
                            {
                                Success = true,
                                Message = "No templates found.",
                                TotalResults = 0,
                                Results = new List<ColorBrandingTemplate>()
                            };
                        }
                    }
                }
            }
            catch (Exception e)
            {
                return new FrameworkResponse
                {
                    Success = false,
                    Message = e.Message
                };
            }
        }